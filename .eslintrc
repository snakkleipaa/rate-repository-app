{
    "plugins": ["react"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
       "requireConfigFile" : "false" 
    }, 
    "babelOptions": {
       "configFile": "./babel.config.js"
    },
    "env": {
      "browser": true
    },
    "rules": {
      "react/prop-types": "off",
      "semi": "error"
    }
  }